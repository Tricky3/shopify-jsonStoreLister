            //console.log("container width is greater than or equal to 1164");
            columnNum  = 4;
        }

		$w -= gutterwidth*(columnNum-1);
		columnWidth = Math.floor($w/columnNum);
		console.log('width '+$w);
		console.log('columnNum '+columnNum);
		console.log('columnWidth '+columnWidth);
		{% if settings.store_locator_order == "masonry" %}
		$('.commonuse').find('.simple-list').each(function() {
			var $item = $(this),
				multiplier_w = $item.attr('class').match(/simple-list(\d)/),
				//multiplier_h = $item.attr('class').match(/simple-list(\d)/),
				width = multiplier_w ? columnWidth*multiplier_w[1]-20 : columnWidth;//,
				//height = multiplier_h ? columnWidth*multiplier_h[1]*0.45-20 : columnWidth*0.45-20;
			$item.css({
				width: width,
				//height: height
			});
		});
		{% endif %}
		return columnWidth;
	};

	//Work for normal masonry
	{% if settings.store_locator_order == "masonry" %}


  		isotope = function () {
  			console.log('isotope as masonry is running');
			$container.isotope({
				isResizeBound: false,
				isFitWidth: false,
				{% unless settings.store_locator_use_selectBox == "none" %}
				itemSelector: '.retailers_info',
				{% endunless %}
				{% if settings.store_locator_use_selectBox == "none" %}
				itemSelector: '.simple-list',
				{% endif %}

				masonry: {
					columnWidth: colWidth(),
					gutter: gutterwidth
				}
			});
		};



	{% else %}

		function processAutoheight(parent,children,positiveFluctuation,negativeFluctuation) {
			var Normalizer = 0;
			var heightverifier = 0;
			var columnVerifier = 1;
			var elementCount = 0;
			var offsetHeight = 0;
    		var maxHeight = -1;
    		var minHeight = 10000; //if it's longer than that, then it's probably a sexual issue that we don't really wanna know about... or do we? :p
    		var totalHeight = 0;

    		parentWidth = $(parent).outerWidth(true); // outerHeight will add padding and margin to height total
       		//console.log(parent+' width '+parentWidth);

    		$(parent).find(children).each(function(){
        		height = $(this).outerHeight(true); // outerHeight will add padding and margin to height total
        		maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
        		minHeight = minHeight < $(this).height() ? minHeight : $(this).height();
        		totalHeight += height;
    		});



            Normalizer =  Math.ceil((totalHeight - (columnNum * maxHeight)) / (columnNum * minHeight));
            Normalizer = Normalizer+positiveFluctuation-negativeFluctuation; //Gaussian Distribution
            Normalizer = Normalizer < 0 ? Normalizer=0 : Normalizer;
            //console.log('Normalizer '+Normalizer);
            var assignedColumnHeightAdjust = 0;
           	var averageColumnHeight = Math.ceil(totalHeight/columnNum);
            
            var assignedColumnHeight = Math.ceil(maxHeight+assignedColumnHeightAdjust+(Normalizer*minHeight)); // Normally distributed

            //Make sure that the averageColumnHeight is bigger than the maxheight of a single element
            if (averageColumnHeight >= maxHeight) {
            	//console.log ('Parent div: '+parent+' averageColumnHeight '+averageColumnHeight+' is bigger than the infimum of maxHeight '+maxHeight);
            }
            else{
            	//console.log ('Parent div: '+parent+' Algorithm problem, averageColumnHeight : '+averageColumnHeight);
            }
            //Make sure that that the assigned Column Height, when multiplied by the number of Columns, does not surpass the Total Height
             if (assignedColumnHeight*columnNum <= totalHeight) {
             	//console.log ('Parent div: '+parent+' ====PROBLEM=====:: assignedColumnHeight : '+assignedColumnHeight+', ('+assignedColumnHeight+'*'+columnNum+'='+assignedColumnHeight*columnNum+')but the totalHeight is SMALLER: '+totalHeight);
             }
             else{
             	//console.log ('Parent div: '+parent+' ====ALL OK=====:: assignedColumnHeight : '+assignedColumnHeight+', ('+assignedColumnHeight+'*'+columnNum+'='+assignedColumnHeight*columnNum+')but the totalHeight is LARGER: '+totalHeight);
             }
            //console.log('assignedColumnHeight ' +assignedColumnHeight+' < averageColumnHeight '+averageColumnHeight);
            //Check if the assigned Colum Height manages to reach the Total Height in LESS columns than the expected
			for (i = 1; i <= columnNum; i++) {
    			var checkTotalAssignedHeight = assignedColumnHeight*i ;
    			//console.log(checkTotalAssignedHeight+' ? '+ totalHeight);
    			if (checkTotalAssignedHeight > totalHeight) {
    				if (i == columnNum) {
    					//console.log('but has happened when we have reached the correc number of columns '+columnNum);
    					//compare the difference to the minimum height of an element 
    					var diff=checkTotalAssignedHeight-totalHeight;
    					if (diff<minHeight){
    						//console.log('Parent div: '+parent+' the difference is LESS than the minimum height('+diff+'px) --> We need to adjust the Assigned Height (was '+assignedColumnHeight+' px)');
    						assignedColumnHeight += minHeight;
    						//console.log('INCREASED by '+minHeight+' and now reached '+assignedColumnHeight);
    					}
    					else{
    						//console.log('Parent div: '+parent+' the difference is MORE than the minimum height ('+diff+'px) --> No need to do any adjustments');
    					}
    				}
    				else{
    					//console.log ('assignedColumnHeight reaches totalHeight in LESS Columns than needed!');
    					//console.log('Parent div: '+parent+' assignedColumnHeight was '+assignedColumnHeight);
    					//make sure that the assigned height always stays positive before adjusting
    					if ((assignedColumnHeight - minHeight <= 0) || assignedColumnHeight - minHeight == minHeight) {
    						// don't do anything
    						//console.log('Parent div: '+parent+' needs to stay at '+assignedColumnHeight);
    					}
    					else{
    						assignedColumnHeight -= minHeight;
    						//console.log('Parent div: '+parent+' decreased by '+minHeight+' and now reached '+assignedColumnHeight);
    					}
	   				}
    			}
			}

			var offsetHeights=[];

            //Check if the assigned Colum Height manages to reach the Total Height in MORE columns than the expected
            $(parent).find(children).each(function(){
        		height = $(this).outerHeight(true); // outerHeight will add padding and margin to height total
        		minHeight = minHeight < $(this).height() ? minHeight : $(this).height();

        		elementCount++;
        		//======================================== verify the height and use the verification for adjusting ========================================
       			if (heightverifier <= assignedColumnHeight){
       				//increase the real height until it reaches the theoretical one, then change column and start again
       				heightverifier = heightverifier + height;
       				if (heightverifier > assignedColumnHeight){
       					//var heightverifierTotal = heightverifier - height;
       					//console.log('catch the height of the column '+ heightverifierTotal +' and store it in an array');
						columnVerifier++;
						if (columnVerifier>columnNum){
							var offsetHeight = +height;
							//console.log('this should not be here columnVerifier: '+columnVerifier+' offset height '+offsetHeight);
							offsetHeights.push(offsetHeight);
        				}
						heightverifier = 0;
       				}
       			} 	
       			
       			//beginning a new column 
        		if (heightverifier==0){
        		 	heightverifier = height;
        		 	//console.log('end the loop with height:'+height );
        		 	//storeThoseHeights();
        		}
      		   	
       		   	//console.log(parent+' ===== '+elementCount+' ===== Columns : '+columnVerifier+' assignedColumnHeight: '+assignedColumnHeight+' heightverifier: '+heightverifier+' (+height '+height+' )');
    		});

			//Grab the offset values and turn them into a sum
			if(offsetHeights.length > 0) {
			    var offsetSum = 0;
				$.each(offsetHeights,function(){offsetSum+=parseFloat(this) || 0;});
				//console.log(offsetSum);
			}
			
			if (columnVerifier>columnNum){
       		   		//console.log('algorithm issue for '+parent+' , columnVerifier: '+columnVerifier+' > columnNum: '+columnNum);
       		   		//console.log('catch the height of the offset columns '+ offsetHeight +' and store it in an array');
       		   		correctAssignedHeight(parent,assignedColumnHeight,minHeight);
       		   	}
       		   	else{
       		   		applyAssignedHeight(parent,assignedColumnHeight);
       		   	}
			
			//console.log('=============////////////============');
	
		}

		function applyAssignedHeight(parent,assignedColumnHeight){
			//console.log('applying the height for'+parent+'.assignedColumnHeight: '+assignedColumnHeight);
			$(parent).height(assignedColumnHeight); // Normally distributed

		}

		function correctAssignedHeight(parent,assignedColumnHeight,minHeight){
			//console.log('adjusting the height for'+parent+'.Initial assignedColumnHeight: '+assignedColumnHeight+'.Adding the minHeight: '+minHeight);
			//add the minimum height until we escape the loop
			//assignedColumnHeight =+(2*minHeight);
			$(parent).height(assignedColumnHeight+minHeight); // Normally distributed

			
			//processAutoheight(parent,'.simple-list',0,0);
		}

		doYourMath = function () {
			colWidth();
			processAutoheight('#homeland','.simple-list',0,0);
			processAutoheight('#overseas','.simple-list',0,0);
			processAutoheight('#onlineStores','.simple-list',0,0);
			{% if settings.store_locator_aggregate_Special %}
			for (var x=0;  x < uniqueSpecialsTotal; x++) {
				//console.log('processing for specialfeats-'+x+'');
				processAutoheight('#specialfeats-' + x + '','.simple-list',0,0);
			}
			{% else %}
			processAutoheight('#specialStores','.simple-list',0,0);
			{% endif %}
		};

		isotope = function () {
			//console.log('isotope fitColumns is running');
			$container.isotope({
				isResizeBound: false,
				itemSelector: '.simple-list',
				isFitWidth: false,
				layoutMode: 'fitColumns',
			});
		};

		doYourMath();

	{% endif %}


  	isotope();
}); // LOAD

var myEfficientFn = debounce(function() {
		//console.log( 'Resize running!===============================');		
		{% if settings.store_locator_order == "fitColumns" %}
		doYourMath();
		{% endif %}
    	isotope();
	}, 250);
window.addEventListener('resize', myEfficientFn);
</script>